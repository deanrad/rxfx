{
  "name": "@rxfx/react",
  "version": "1.1.3",
  "license": "MIT",
  "author": "Dean Radcliffe",
  "repository": "https://github.com/deanrad/rxfx",
  "main": "dist/rxfx-react.min.js",
  "module": "dist/rxfx-react.esm.js",
  "typings": "dist/tsc/types/src/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "alias": {
    "process": false
  },
  "scripts": {
    "start": "yarn test:fast",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts --fix .",
    "test": "yarn test:fast",
    "test:fast": "jest",
    "test:watch": "jest --verbose=false --watch",
    "test:coverage": "jest --coverage",
    "test:quiet": "jest --silent --reporters=jest-silent-reporter",
    "test:full": "yarn test:fast && yarn test:quiet && echo 'Retest successful' && yarn lint && yarn build",
    "doc": "typedoc --out dist/docs/react",
    "doctoc": "doctoc .",
    "build": "rimraf dist && tsc && timeout --foreground --preserve-status 10 rollup --bundleConfigAsCjs -c rollup.config.js"
  },
  "dependencies": {
    "@rxfx/bus": "workspace:^",
    "@rxfx/effect": "workspace:^",
    "@rxfx/service": "workspace:^",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@open-wc/building-rollup": ">= 1.10.0",
    "@rollup/plugin-typescript": ">= 8.2.5",
    "@rxfx/service": "workspace:^",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": ">= 27.0.1",
    "@types/react": "^18.2.38",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "babel-jest": ">= 27.0.6",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": ">= 24.4.2",
    "husky": ">= 7.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-silent-reporter": ">= 0.5.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": ">= 3.0.2",
    "rollup": "^3.29.4",
    "typedoc": "^0.25.3",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "react": ">=18.2.0"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
