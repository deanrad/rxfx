{
  "name": "@rxfx/fsa",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Dean Radcliffe",
  "repository": "https://github.com/deanrad/rxfx",
  "main": "dist/rxfx-fsa.min.js",
  "module": "dist/rxfx-fsa.esm.js",
  "typings": "dist/tsc/types/src/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "alias": {
    "process": false
  },
  "scripts": {
    "start": "yarn test:fast",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts --fix .",
    "test": "yarn test:fast",
    "test:fast": "jest",
    "test:jsdom": "jest -c jsdom.jest.config.js",
    "test:watch": "jest --verbose=false --watch",
    "test:coverage": "jest --coverage",
    "test:quiet": "jest --silent --reporters=jest-silent-reporter",
    "test:full": "yarn test:fast && yarn test:jsdom && yarn test:quiet && echo 'Retest successful' && yarn lint && yarn build",
    "doc": "typedoc --out dist/docs/fsa",
    "doctoc": "doctoc .",
    "build": "rimraf dist && tsc && timeout --foreground --preserve-status 10 rollup -c rollup.config.js"
  },
  "devDependencies": {
    "@babel/core": ">= 7.15.0",
    "@babel/preset-env": ">= 7.15.0",
    "@babel/preset-typescript": ">= 7.15.0",
    "@open-wc/building-rollup": ">= 1.10.0",
    "@rollup/plugin-typescript": ">= 8.2.5",
    "@size-limit/preset-small-lib": ">= 5.0.3",
    "@types/jest": ">= 27.0.1",
    "@typescript-eslint/eslint-plugin": ">= 4.31.2",
    "@typescript-eslint/parser": ">= 4.31.2",
    "babel-jest": ">= 27.0.6",
    "deepmerge": ">= 4.2.2",
    "eslint": ">= 7.32.0",
    "eslint-plugin-jest": ">= 24.4.2",
    "jest": ">= 27.0.6",
    "jest-silent-reporter": ">= 0.5.0",
    "prettier": ">= 2.6.2",
    "rimraf": ">= 3.0.2",
    "rollup": ">= 2.56.2",
    "size-limit": ">= 5.0.3",
    "tiny-invariant": ">= 1.1.0",
    "typedoc": "latest",
    "typescript": ">= 4.3.5"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
